{
  "private": true,
  "name": "clientapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build --no-esm",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "yarn install && preact watch --no-esm && echo Starting the development server",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "jest ./tests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged" 
    }
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@types/google-map-react": "1.1.8",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "2.2.1",
    "google-map-react": "1.1.7",
    "preact": "10.4.4",
    "preact-cli-plugin-env-vars": "1.2.1",
    "preact-markup": "2.0.0",
    "preact-render-to-string": "5.1.9",
    "preact-router": "3.2.1",
    "rbx": "2.2.0",
    "react-geocode": "0.2.1",
    "react-use-googlelogin": "0.12.5"
  },
  "devDependencies": {
    "@types/jest": "26.0.0",
    "@types/react-geocode": "0.2.0",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "css-loader": "1.0.1",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-preset-preact": "1.0.0",
    "lint-staged": "10.2.10",
    "preact-cli": "3.0.0-rc.14",
    "prettier": "2.0.5",
    "sirv-cli": "1.0.0",
    "typescript": "3.9.7",
    "typings-for-css-modules-loader": "1.7.0"
  }
}
